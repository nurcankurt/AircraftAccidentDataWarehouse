      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{DE649012-6348-411E-8545-9310DF174577}"
          SQLTask:SqlStatementSource="WITH SplitValues AS (&#xA;    SELECT &#xA;        finding_description,&#xA;        CHARINDEX('-', finding_description) AS firstDash,&#xA;        CHARINDEX('-', finding_description, CHARINDEX('-', finding_description) + 1) AS secondDash,&#xA;        CHARINDEX('-', finding_description, CHARINDEX('-', finding_description, CHARINDEX('-', finding_description) + 1) + 1) AS thirdDash,&#xA;        CHARINDEX('-', finding_description, CHARINDEX('-', finding_description, CHARINDEX('-', finding_description, CHARINDEX('-', finding_description) + 1) + 1) + 1) AS fourthDash&#xA;    FROM [dbo].[Finding]&#xA;)&#xA;SELECT &#xA;    finding_description,&#xA;    CASE WHEN LEN(finding_description) - LEN(REPLACE(finding_description, '-', '')) &gt;= 1 THEN SUBSTRING(finding_description, 1, firstDash - 1) ELSE NULL END AS category,&#xA;    CASE WHEN LEN(finding_description) - LEN(REPLACE(finding_description, '-', '')) &gt;= 2 THEN SUBSTRING(finding_description, firstDash + 1, secondDash - firstDash - 1) ELSE NULL END AS subcategory,&#xA;    CASE WHEN LEN(finding_description) - LEN(REPLACE(finding_description, '-', '')) &gt;= 3 THEN SUBSTRING(finding_description, secondDash + 1, thirdDash - secondDash - 1) ELSE NULL END AS section,&#xA;    CASE WHEN LEN(finding_description) - LEN(REPLACE(finding_description, '-', '')) &gt;= 4 THEN SUBSTRING(finding_description, thirdDash + 1, fourthDash - thirdDash - 1) ELSE NULL END AS subsection,&#xA;    CASE WHEN LEN(finding_description) - LEN(REPLACE(finding_description, '-', '')) &gt;= 5 THEN SUBSTRING(finding_description, fourthDash + 1, LEN(finding_description)) ELSE NULL END AS modifier&#xA;FROM&#xA;    SplitValues;&#xA;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
      </DTS:ObjectData>
    </DTS:Executable>
